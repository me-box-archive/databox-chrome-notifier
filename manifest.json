{
    "manifest_version": 2,
    "name": "Databox Notifier",
    "description": "Receive notifications from databox",
    "version": "0.1",
    "permissions": ["gcm", "storage", "notifications"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    }

}